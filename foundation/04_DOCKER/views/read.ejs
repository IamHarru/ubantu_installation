<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/styles/style.css">
    <script src=""></script>
    <script src="/javascripts/script.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@tailwindcss/browser@4"></script>
    <title>Document</title>
</head>

<body style="background-image: url(/images/Gemini_Generated_Image_hntspehntspehnts.png),url(https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS5IgSIji6DIroSwL-vniwnPXpceIBpbSRg7Q&s);  background-repeat: no-repeat;
    background-size: cover;
    background-attachment: fixed;">
    <div class="container_1">
        <h1>* Created Users</h1>
    </div>
    <div class="pcontainer">

        <%users.forEach(function(user) {%>
            <div class="container_3">
                <div class="boxi">
                    <div class="boxc">
                        <img src="<%=user.image%>" alt="Iron Man"
                            style="width: 100%; height: 200px; object-fit: cover; border-radius: 7px;">
                    </div>

                </div>
                <div class="boxd">
                    <h1>
                        name : <%=user.name%>
                    </h1>
                    <p>
                        id : <%=user._id%>
                    </p>
                    <p>
                        gender: <%=user.gender%>
                    </p>

                    <p>
                        gmail: <%=user.gmail%>
                    </p>

                </div>
                <div class="boxu">
                    <a class="aed" href="/edit/<%= user._id%>">edit</a>
                    <a class="ade" href="/delete/<%= user._id%>">delete</a>
                </div>
                <!-- <img src="<%=user.image%>" alt=""> -->
            </div>
            <%})%>

    </div>
    <div class="container_2">
        <a href="/">Go Back TO Home</a>
    </div>

</body>

</html>